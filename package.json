{
  "name": "tab-picker",
  "displayName": "Tab Picker",
  "description": "A tab picker inspired by Barbar (Neovim)",
  "version": "0.0.0",
  "publisher": "haavardnk",
  "author": "Håvard Kråkenes",
  "license": "MIT",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "tab navigation",
    "buffer picker",
    "quick tab navigation",
    "neovim buffer"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Tab Picker",
      "properties": {
        "tabPicker.keyAssignmentStrategy": {
          "type": "string",
          "default": "filename",
          "enum": [
            "filename",
            "leftHand",
            "rightHand",
            "homeRow"
          ],
          "enumDescriptions": [
            "Assign keys based on letters in the filename (e.g., 'tabs.ts' gets 't')",
            "Prioritize keys on the left side of the keyboard (a, s, d, f, q, w, e, r, etc.)",
            "Prioritize keys on the right side of the keyboard (j, k, l, h, u, i, o, p, etc.)",
            "Prioritize home row keys first (a, s, d, f, g, h, j, k, l, then q, w, e, r, etc.)"
          ],
          "description": "Strategy for assigning keyboard shortcuts to tabs"
        }
      }
    },
    "commands": [
      {
        "command": "tab-picker.pickTab",
        "title": "Tab Picker: Pick a Tab"
      },
      {
        "command": "tab-picker.deleteTab",
        "title": "Tab Picker: Delete a Tab"
      },
      {
        "command": "tab-picker.cancel",
        "title": "Tab Picker: Cancel"
      },
      {
        "command": "tab-picker.cleanup",
        "title": "Tab Picker: Force Cleanup Labels"
      },
      {
        "command": "tab-picker.keyPress",
        "title": "Tab Picker: Key Press (Internal)"
      }
    ],
    "keybindings": [
      {
        "command": "tab-picker.pickTab",
        "key": "ctrl+'",
        "mac": "ctrl+'"
      },
      {
        "command": "tab-picker.deleteTab",
        "key": "ctrl+shift+'",
        "mac": "ctrl+shift+'"
      },
      {
        "command": "tab-picker.cancel",
        "key": "escape",
        "mac": "escape",
        "when": "tabPickerActive"
      },
      {
        "command": "tab-picker.keyPress",
        "key": "a",
        "args": {
          "key": "a"
        },
        "when": "tabPickerActive"
      },
      {
        "command": "tab-picker.keyPress",
        "key": "b",
        "args": {
          "key": "b"
        },
        "when": "tabPickerActive"
      },
